apiVersion: v1
kind: ConfigMap
metadata:
  name: {{item.name}}
data:
  name: {{item.name}} 
  sendReceiveUrl: {{item.sendReceiveUrl}}
  messageSize: "{{item.messageSize}}"
  testDuration: "{{item.testDuration}}"
  rate: "{{item.rate}}"
  testCase: {{item.testCase}}
  warmUp: "true"
  initialRate: "{{item.initialRate}}"
  ceilingRate: "{{item.ceilingRate}}"
  rateIncrement: "{{item.rateIncrement}}"
  initalParallelCount: "{{item.initalParallelCount }}"
  ceilingParallelCount: "{{item.ceilingParallelCount }}"
  parallelCountIncrement: "{{item.parallelCountIncrement }}"
  maxLatency: "{{item.maxLatency}}"
  maestro-cli.properties: |
        warm-up.message.count=10000
        warm-up.rate.percent=10
        warm-up.ceiling.parallel.count=1
        maestro.worker.stop.timeout=60000


